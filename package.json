{
  "name": "prp",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "sr_": "ng serve review --port=4002",
    "sr": "npm-run-all -p electron:serve sr_",
    "electron:serve": "wait-on http-get://localhost:4002/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "be": "npm run electron:serve-tsc && ng build review --base-href ./",
    "br__": "npm run be -- -c production",
    "br_": "npm run br__ && electron-builder build --windows --x64 && npm run c2r",
    "br": "npm run sv && npm run br_",
    "build-app": "npm run br__ && electron-builder build --windows --x64"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^11.0.5",
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.0.5",
    "@angular/localize": "^11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@angular/service-worker": "~11.0.5",
    "@types/jwt-decode": "^2.2.1",
    "@types/string-similarity": "^4.0.0",
    "fs-extra": "^9.1.0",
    "rxjs": "~6.5.4",
    "string-similarity": "^4.0.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "9.1.0",
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@angular/language-service": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "devtron": "1.4.0",
    "electron": "^8.2.5",
    "electron-builder": "22.6.0",
    "electron-reload": "1.5.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "npm-run-all": "4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5",
    "wait-on": "4.0.2"
  }
}
